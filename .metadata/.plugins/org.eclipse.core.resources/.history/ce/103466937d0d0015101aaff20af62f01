$(function (){
	
	function init(){
		localStorage.clear();
		console.log("iniciando...");
	}

	$("#btn-login").click(function(){
		var email = $("#Email").val();
		var senha = $("#Senha").val();
		if (email != "" && senha != ""){
			localStorage["login"] = btoa(email + "/" + senha );
		} 
	})
	 


	init();
});

